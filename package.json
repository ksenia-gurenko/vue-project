    "primeflex": "^4.0.0",
    "primeicons": "^6.0.1",
    "primevue": "^3.39.0",
    "vue": "^3.5.18",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tsconfig/node20": "^20.1.6",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.17.2",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
